<configuration>
  <system.webServer>
    <!--https://www.iis.net/configreference/system.webserver/modules-->
    <modules  runAllManagedModulesForAllRequests="true">
      <add name="LegacyHandler" type="LegacyJS.LegacyHandler, LegacyJS" preCondition="managedHandler"/>
    </modules>
  </system.webServer>
  <system.web>
    <compilation debug="true" targetFramework="4.5.2"/>
    <httpRuntime targetFramework="4.5.2"/>
  </system.web>
  <system.codedom>
  </system.codedom>
</configuration>